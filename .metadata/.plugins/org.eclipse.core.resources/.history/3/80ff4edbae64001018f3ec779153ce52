//using para constructor...
import java.util.*;
public class uDemo2 extends Exception
{
	void checkAge()
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Age-");
		int age=sc.nextInt();
		try
		{
		if(age<0)
		{
			throw new NegativeAgeException("Age is Negative....");
			
		}
		}catch(Exception e)
		{
			System.out.println(e);
		}
	}
	public static void main(String[] args)
	{
	 uDemo2 ob=new uDemo2();
	 ob.checkAge();
	}

}
