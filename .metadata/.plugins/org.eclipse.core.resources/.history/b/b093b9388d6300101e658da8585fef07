public class Demo
{
	public static void main(String[] args)
	{
	 int a[]={10,20,30,40,50};
	 for(int value:a)
	 {
		 System.out.println(value);
	 }
	 int b[]=new int[5];
     b[0]=1;
     b[1]=2;
     b[2]=3;
     b[3]=4;
     b[4]=5;
     for(int i=0;i<b.length;i++)
	 {
		 System.out.println(i);
	 }
	 
	}

}
