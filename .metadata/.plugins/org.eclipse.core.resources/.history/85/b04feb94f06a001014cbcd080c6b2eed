import java.util.*;
public class LinkedList1
{
	public static void main(String[] args)
	{
	LinkedList l1=new LinkedList();
	l1.add(11);
	l1.add(34);
	l1.add(67);
	l1.add(89);
	System.out.println(l1);
	l1.addFirst(10);
	l1.addLast(100);
	l1.removeLast();
	System.out.println(l1);
	System.out.println("First no-"+l1.getFirst());
	System.out.println("Last no-"+l1.getLast());
	System.out.println("Index of 67-"+l1.indexOf(67));
	l1.poll();
	System.out.println(l1);
	l1.pop();               //remove first
	System.out.println(l1);
	l1.push(101);   //add at first
	System.out.println(l1);
	Collections.reverse(l1);
	System.out.println("Reverse Linked List-"+l1);
	
	LinkedList l2=new LinkedList();
	l2.add("om");
	l2.add("sai");
	l2.add("ram");
	l2.add("sham");
	l2.add("aman");
	System.out.println(l2);
	System.out.println("After adding l1-"+l2);
	
	
	}

}
