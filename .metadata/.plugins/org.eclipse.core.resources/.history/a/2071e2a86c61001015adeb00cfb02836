
public class D extends C 
{
  @Override
void login(String username)
  {
	System.out.println("Login By-"+username);  
  }
@Override
	void login(String username, int password)
{
	System.out.println("Login By-"+username+"&"+password);		
	}
@Override
	void login(long adhar) 
{
	System.out.println("Login By-"+adhar);		
	}

@Override
void register(String username)
{
	System.out.println(username);
}


	public static void main(String[] args) 
	{
	 D ob=new D();
	 ob.login("Bharat");
	 ob.login("Bharat",1234);
	 ob.login(987765456654567L);
	 ob.register("om");
	}

}
